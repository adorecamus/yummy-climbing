<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yummyclimbing.mapper.community.CommunityFileInfoMapper">

<sql id="cfColumns">
	CF_NUM, CF_ORIG_NAME, CF_LOGI_NAME, CF_LOGI_PATH,
	CB_NUM, CF_EXT, CF_SIZE, CF_CREDAT
</sql>

<insert id="insertFile" parameterType="list">
	INSERT INTO COMMUNITY_FILE_INFO(
	<include refid="cfColumns"/>
	)
	VALUES
	<foreach collection="list" item="item" separator=",">
	(
	#{item.cfNum}, #{item.cfOrigName}, #{item.cfLogiName}, #{item.cfLogiPath},
	#{item.cbNum}, #{item.cfExt}, #{item.cfSize},#{item.cfCredat},	
	)
	</foreach>
</insert>

</mapper>